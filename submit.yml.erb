<%-  

  hours = time.to_i*60
  slurm_args = [ "--nodes", "1", "--ntasks-per-node", "1", "--mem", "4096", "--cpus-per-task", "1", "--time", "#{hours}" ]
%>
---
batch_connect:
  template: vnc
script:
  accounting_id: "<%= account %>"
  native:
  <%- slurm_args.each do |arg| %>
    - "<%= arg %>"
  <%- end %>