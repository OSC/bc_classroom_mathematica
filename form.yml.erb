---
cluster:
  - "kubernetes"
  - "kubernetes-test"
  - "kubernetes-dev"
form:
  - account
  - compute_cluster
  - time
  - size
attributes:
  size:
    widget: "hidden_field"
    value: "standard"
  account:
    widget: "select"
    options:
      - [
        "PZS0714", "PZS0714",
        ]
  compute_cluster:
    widget: "select"
    options:
      - [
        "pitzer", "pitzer"
        ]
  time:
    widget: "hidden_field"
    value: "1.5"